    cowplot,
    data.table,
    dplyr,
    furrr,
    future.apply,
    ggplot2,
    glue,
    gridExtra,
    gtools,
    HGNChelper,
    magrittr,
    purrr,
    rlang,
    Seurat,
    SeuratBubblePlot,
    stringr,
    tibble
Remotes:
    milescsmith/SeuratBubblePlot
Encoding: UTF-8
LazyData: true
RoxygenNote: 6.1.1
